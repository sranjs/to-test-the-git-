          "Complex_NetAmountWithShipAndTaxes" : [ {
            "sql" : "SELECT i_item_sk ws_item_sk, \nword_order_id ws_order_number, \n((wlin_sales_price * wlin_quantity)-wlin_coupon_amt + (wlin_ship_cost * wlin_quantity)) \n* (1 + web_tax_percentage) as WS_NET_PAID_INC_SHIP_TAX \nFROM \ns_web_order \nJOIN s_web_order_lineitem ON (word_order_id = wlin_order_id) \nLEFT OUTER JOIN item ON (wlin_item_id = i_item_id AND i_rec_end_date IS NULL)  \nLEFT OUTER JOIN web_site ON (word_web_site_id = web_site_id AND web_rec_end_date IS NULL) order by ws_item_sk, ws_order_number \n",
            "name" : "Complex_NetAmountWithShipAndTaxes"
          } ],
          "Medium_TotalDiscountReceived" : [ {
            "sql" : "SELECT (i_item_sk) as ws_item_sk, \n(word_order_id)  as ws_order_number, \n(i_current_price-wlin_sales_price)*wlin_quantity ws_ext_discount_amt \nFROM \ns_web_order \nJOIN s_web_order_lineitem ON (word_order_id = wlin_order_id) \nLEFT OUTER JOIN item ON (wlin_item_id = i_item_id AND i_rec_end_date IS NULL)  \nLEFT OUTER JOIN web_site ON (word_web_site_id = web_site_id AND web_rec_end_date IS NULL) order by ws_item_sk, ws_order_number",
            "name" : "Medium_TotalDiscountReceived"
          } ],
          "Medium_NetAmountWithTax" : [ {
            "sql" : "SELECT (i_item_sk) as ws_item_sk, \n(word_order_id)  as ws_order_number, \n((wlin_sales_price * wlin_quantity)-wlin_coupon_amt)*(1+web_tax_percentage) ws_net_paid_inc_tax \nFROM \ns_web_order \nJOIN s_web_order_lineitem ON (word_order_id = wlin_order_id) \nLEFT OUTER JOIN item ON (wlin_item_id = i_item_id AND i_rec_end_date IS NULL)  \nLEFT OUTER JOIN web_site ON (word_web_site_id = web_site_id AND web_rec_end_date IS NULL) order by ws_item_sk, ws_order_number",
